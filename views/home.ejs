  <body>
    <div class="navbar-fixed">
      <nav class="#006064 cyan darken-4">
        <div class="nav-wrapper">
          <a href="#!" class="brand-logo center">SURVEY WEB_APP SIH 2020</a>
          <ul class="right hide-on-med-and-down">
            <li>
              <a href="https://ruchir-bisht-007.netlify.com/"
                >About Devlopers</a
              >
            </li>
            <li>
              <a
                href="https://www.sih.gov.in/sih2020PS/MTA=/QWxs/QWxs/QWxs?page=2"
                >About the Project</a
              >
            </li>
          </ul>
        </div>
      </nav>
    </div>
    <div class="modal fade" id="myModal" role="dialog">
      <div class="modal-dialog">
        <!-- Modal content-->
        <div class="modal-content">
          <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal">
              &times;
            </button>
            <center><h2 class="modal-title">SURVEY RESULT</h2></center>
          </div>
          <div class="modal-body">
            <div id="myDiv" width="100" height="100">
              <!-- Plotly chart will be drawn inside this DIV -->
            </div>
            <p id="ratingTab1"></p>
            <p id="ratingTab2"></p>
            <p id="ratingTab3"></p>
            <p id="ratingTab4"></p>
            <p id="ratingTab5"></p>
          </div>
          <div class="modal-footer">
            <b><h4 id="total_score"></h4></b>
            <button type="button" class="btn btn-success" data-dismiss="modal">
              Close
            </button>
          </div>
        </div>
      </div>
    </div>
    <center>
      <div id="loading" style="display: none;visibility: hidden;">
        <img src="./images/loading1.gif" alt="Loading ... " />
      </div>
      <h2 id="display-name"></h2>
      <h4 id="display-email"></h4>
      <div id="login-form">
        <div>
          <div class="input-field col s3">
            <input id="email" type="email" class="validate" />
            <label for="email">Email</label>
          </div>
          <div class="input-field col s3">
            <input id="password" type="password" class="validate" />
            <label for="password">Password</label>
          </div>
        </div>
        <button
          class="btn waves-effect waves-red green"
          id="sign_in"
          onclick="{SignInWithEmailAndPassword()}"
        >
          Login
        </button>
      </div>
      <br />
      <div id="logout-form" style="display: none;visibility: hidden;">
        <button
          class="btn waves-effect waves-red"
          id="sign_out"
          style="display: none;visibility: hidden;"
          onclick="{SignOutWithEmailAndPassword()}"
        >
          Logout
        </button>
      </div>
      <br /><br />
      <button
        class="btn waves-effect waves-red"
        id="transaction"
        onclick="{show_results()}"
        style="display: none;visibility: hidden;"
      >
        SHOW SURVEYS
      </button>
      <div id="all_transaction" style="display: none;visibility: hidden;">
        <h3 id="device_ID"></h3>
        <div id="list"></div>
      </div>
      <br /><br />
      <div id="loading" style="display: none;visibility: hidden;">
        <img src="./images/loading1.gif" alt="Loading ... " />
      </div>
    </center>
    <!-- footer -->
    <br /><br />
